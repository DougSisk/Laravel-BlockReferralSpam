{
    "name": "dougsisk/laravel-block-referral-spam",
    "description": "Blocks referral spam using middleware for Laravel 5+",
    "keywords": ["laravel","referer","referral","spam","block","middleware"],
    "homepage": "https://github.com/DougSisk/Laravel-BlockReferralSpam",
    "license": "MIT",
    "authors": [
        {
            "name": "Doug Sisk",
            "homepage": "http://dougsisk.com"
        }
    ],
    "support": {
        "issues": "https://github.com/DougSisk/Laravel-BlockReferralSpam/issues"
    },
    "require": {
        "php": ">=5.4.0",
        "illuminate/http": "^5.0",
        "piwik/referrer-spam-blacklist": "dev-master"
    },
    "require-dev": {
        "orchestra/testbench": "~3.0",
        "phpunit/phpunit": "~4.0|~5.0"
    },
    "autoload": {
        "psr-4": {
            "DougSisk\\BlockReferralSpam\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}